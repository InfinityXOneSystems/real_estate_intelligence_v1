# ============================================================================
# REAL ESTATE INTELLIGENCE - ENVIRONMENT CONFIGURATION
# Version 1.0.0 - Intelligent Memory & LLM Integration
# ============================================================================

# ============================================================================
# APPLICATION CORE
# ============================================================================
NODE_ENV=production
PORT=4000
LOG_LEVEL=info
APP_VERSION=1.0.0

# ============================================================================
# DATABASE - PostgreSQL (Docker)
# ============================================================================
DB_HOST=postgres
DB_PORT=5432
DB_NAME=real_estate_intelligence
DB_USER=postgres
DB_PASSWORD=your_secure_postgres_password_here
DATABASE_URL=postgresql://postgres:your_secure_postgres_password_here@postgres:5432/real_estate_intelligence

# ============================================================================
# CACHE - Redis (Docker)
# ============================================================================
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_secure_redis_password_here

# ============================================================================
# GOOGLE CLOUD - FIRESTORE & VERTEX AI
# ============================================================================

# Service Accounts
GCP_PROJECT_ID=infinity-x-one-systems
GCP_SERVICE_ACCOUNT_EMAIL=real-estate-intelligence@infinity-x-one-systems.iam.gserviceaccount.com
GCP_SERVICE_ACCOUNT_KEY_PATH=./secrets/gcp-service-account.json
GCP_PRIVATE_KEY=your_gcp_private_key_here
GCP_REGION=us-east1

# Firestore Configuration
FIRESTORE_PROJECT_ID=infinity-x-one-systems
FIRESTORE_SERVICE_ACCOUNT=real-estate-intelligence@infinity-x-one-systems.iam.gserviceaccount.com
FIRESTORE_DATABASE=real-estate-intelligence-db

# Google Cloud Storage (GCS)
GCS_BUCKET=infinity-x-one-systems
GCS_SUBFOLDER=real-estate-intelligence
GCS_ARCHIVE_PREFIX=archive
GCS_TRANSACTION_PREFIX=transactions
GCS_CRAWLED_DATA_PREFIX=crawled-data
GCS_REPORTS_PREFIX=reports
GCS_TRAINING_DATA_PREFIX=training-data
GCS_AUDIT_LOGS_PREFIX=audit-logs

# Vertex AI Configuration
VERTEX_AI_API_KEY=your_vertex_ai_api_key_here
VERTEX_AI_SERVICE_ACCOUNT=vertex-express@infinity-x-one-systems.iam.gserviceaccount.com
VERTEX_AI_REGION=us-east1
VERTEX_AI_MODELS=gemini-2-0-pro,gemini-2-0-flash,gemini-1-5-pro,text-embedding-004,code-gemma-7b

# ============================================================================
# LANGUAGE MODELS - Multi-Provider Configuration
# ============================================================================

# Anthropic (Claude Models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=2048
CLAUDE_TEMPERATURE=0.7

# Google Gemini (Direct API)
GOOGLE_GEMINI_KEY=your_google_gemini_api_key_here
GEMINI_MODEL=gemini-2-0-pro
GEMINI_MAX_TOKENS=4096
GEMINI_TEMPERATURE=0.7

# ============================================================================
# RAG MEMORY SERVICE
# ============================================================================
RAG_MEMORY_ENABLED=true
RAG_EMBEDDING_MODEL=text-embedding-004
RAG_CACHE_ENABLED=true
RAG_CACHE_TTL_HOURS=24
RAG_MIN_CONFIDENCE_SCORE=0.7
RAG_MAX_CONTEXT_RESULTS=10
RAG_BATCH_SIZE=100

# ============================================================================
# EXTERNAL APIs - Real Estate Data
# ============================================================================

# Stripe (Payment Processing)
STRIPE_SECRET_KEY=sk_test_YOUR_STRIPE_SECRET_KEY_HERE
STRIPE_PUBLIC_KEY=pk_test_YOUR_STRIPE_PUBLIC_KEY_HERE
STRIPE_WEBHOOK_SECRET=whsec_YOUR_WEBHOOK_SECRET_HERE

# Zillow/Realtor APIs
ZILLOW_API_KEY=your_zillow_api_key_here
REALTOR_API_KEY=your_realtor_api_key_here
REDFIN_API_KEY=your_redfin_api_key_here

# Government Data APIs
CENSUS_API_KEY=your_census_api_key_here
HUD_API_KEY=your_hud_api_key_here
COUNTY_RECORDER_API_KEY=your_county_recorder_api_key_here

# Market Data APIs
ZILLOW_MARKET_API_KEY=your_zillow_market_api_key_here
COSTAR_API_KEY=your_costar_api_key_here

# Social Media & Web Crawling
TWITTER_API_KEY=your_twitter_api_key_here
FACEBOOK_API_KEY=your_facebook_api_key_here
LINKEDIN_API_KEY=your_linkedin_api_key_here
SCRAPER_API_KEY=your_scraper_api_key_here

# ============================================================================
# BLOCKCHAIN - Solidity Contracts
# ============================================================================
HARDHAT_NETWORK=localhost
HARDHAT_PRIVATE_KEY=your_hardhat_private_key_here
HARDHAT_MNEMONIC=your_hardhat_mnemonic_here
ESCROW_CONTRACT_ADDRESS=your_escrow_contract_address_here
WEB3_PROVIDER_URL=http://localhost:8545

# Cryptocurrency APIs
COINBASE_API_KEY=your_coinbase_api_key_here
ETHEREUM_RPC_URL=your_ethereum_rpc_url_here

# ============================================================================
# AUTHENTICATION & SECURITY
# ============================================================================
JWT_SECRET=your_jwt_secret_key_here_min_32_characters
SESSION_SECRET=your_session_secret_here
ENCRYPTION_KEY=your_encryption_key_here
BCRYPT_ROUNDS=12

# ============================================================================
# MONITORING & LOGGING
# ============================================================================
WINSTON_LOG_LEVEL=info
SENTRY_DSN=your_sentry_dsn_here
DATADOG_API_KEY=your_datadog_api_key_here
DATADOG_APP_KEY=your_datadog_app_key_here

# ============================================================================
# WORKFLOW & AUTOMATION
# ============================================================================
CRON_MARKET_ANALYSIS_SCHEDULE=0 */6 * * *
CRON_CRAWLER_SCHEDULE=0 */4 * * *
CRON_MEMORY_CLEANUP_SCHEDULE=0 3 * * 0
CRON_REPORT_GENERATION_SCHEDULE=0 2 * * *
MAX_WORKERS=8
TASK_TIMEOUT_MS=300000

# ============================================================================
# AGENT CONFIGURATION
# ============================================================================
AGENT_ORCHESTRATION_ENABLED=true
AGENT_AUTO_HEALING_ENABLED=true
AGENT_LEARNING_ENABLED=true
AGENT_MEMORY_PERSISTENCE=firestore
AGENT_MAX_CONCURRENT=10
AGENT_TIMEOUT_MS=180000

# ============================================================================
# FEATURE FLAGS
# ============================================================================
FEATURE_FIRESTORE_MEMORY=true
FEATURE_RAG_RETRIEVAL=true
FEATURE_MULTI_MODEL_LLM=true
FEATURE_GCS_PERSISTENCE=true
FEATURE_VERTEX_AI=true
FEATURE_CLAUDE_INTEGRATION=true
FEATURE_GEMINI_INTEGRATION=true
FEATURE_AUTO_LEARNING=true
FEATURE_SMART_ROUTING=true
FEATURE_EMOTIONAL_PREDICTION=true
FEATURE_HEATMAP_ANALYSIS=true

# ============================================================================
# DEPLOYMENT
# ============================================================================
DEPLOYMENT_ENV=production
DEPLOYMENT_REGION=us-east1
DEPLOYMENT_TIMEOUT=600
DOCKER_REGISTRY=docker.io
GITHUB_TOKEN=your_github_token_here

# ============================================================================
# NOTIFICATION & ALERTING
# ============================================================================
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
EMAIL_SERVICE_API_KEY=your_email_service_api_key_here
ALERT_THRESHOLD_ERROR_RATE=0.05
ALERT_THRESHOLD_LATENCY_MS=5000

# ============================================================================
# TESTING & DEVELOPMENT
# ============================================================================
TEST_MODE=false
MOCK_EXTERNAL_APIS=false
SEED_TEST_DATA=false
DEBUG_MODE=false
VERBOSE_LOGGING=false
